.
.SH "EXAMPLES"
.
.nf

$ git feat dependencies
\.\.\.
$ (feat/dependencies) git commit \-m "Some changes"
\.\.\.
$ (feat/dependencies) git checkout master
$ git feat finish dependencies

$ git alias feats "feat \-a feats"
